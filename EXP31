#include <stdio.h>
#include <ctype.h>

int main() {
    char ch;
    int chars = 0, words = 0, lines = 0;
    int inWord = 0;

    while ((ch = getchar()) != EOF) {
        chars++;
        if (ch == '\n') lines++;
        if (isspace(ch)) inWord = 0;
        else if (!inWord) { inWord = 1; words++; }
    }

    printf("Characters: %d\nWords: %d\nLines: %d\n", chars, words, lines);
    return 0;
}
