#include <stdio.h>
#include <string.h>

char input[100];
int pos = 0;

void E();
void Eprime();
void T();

void match(char t) {
    if (input[pos] == t) pos++;
    else {
        printf("Syntax Error!\n");
        exit(0);
    }
}

void E() {
    T();
    Eprime();
}

void Eprime() {
    if (input[pos] == '+') {
        match('+');
        T();
        Eprime();
    }
}

void T() {
    if (input[pos] == 'i' && input[pos+1] == 'd')
        pos += 2;
    else {
        printf("Syntax Error!\n");
        exit(0);
    }
}

int main() {
    printf("Enter string: ");
    scanf("%s", input);
    E();
    if (input[pos] == '\0')
        printf("Parsing successful.\n");
    else
        printf("Parsing failed.\n");
    return 0;
}
